{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "llms/login",
  "title": "LifterLMS Login",
  "icon": "migrate",
  "category": "lifterlms",
  "description": "Displays the LifterLMS login form. If a user is already logged in, nothing is displayed.",
  "textdomain": "lifterlms",
  "attributes": {
    "layout": {
      "type": "string",
      "default": "columns"
    },
    "redirect": {
      "type": "string"
    },
    "llms_visibility": {
      "type": "string"
    },
    "llms_visibility_in": {
      "type": "string"
    },
    "llms_visibility_posts": {
      "type": "string"
    }
  },
  "supports": {
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "render": "file:../../templates/blocks/shortcode.php"
}